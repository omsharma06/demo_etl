package routines;

public class PasswordRoutine {

    public static String encryptPassword(String e_password) {
        if (e_password == null) {
            return new String("NULL");
        }
        else {
        	return new String(DataMasking.encryptAESGCM(e_password, "sN0wTalendTR@c21"));
        }
    }   
    
    public static String decryptPassword(String d_password) {
        if (d_password == null) {
            return new String("NULL");
        }
        else {
        	return new String(DataMasking.decryptAESGCM(d_password, "sN0wTalendTR@c21"));
        }
    }  
}
